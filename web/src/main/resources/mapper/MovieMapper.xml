<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xupt.dao.MovieMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xupt.pojo.Movie">
        <id column="id" property="id" />
        <result column="movie_name" property="movieName" />
        <result column="movie_status" property="movieStatus" />
        <result column="movie_start" property="movieStart" />
        <result column="movie_minute" property="movieMinute" />
        <result column="movie_brief" property="movieBrief" />
        <result column="movie_score" property="movieScore" />
        <result column="movie_money" property="movieMoney" />
        <result column="movie_area" property="movieArea" />
        <result column="movie_type" property="movieType" />
        <result column="movie_head" property="movieHead" />
        <result column="day_money" property="dayMoney" />
        <result column="want_look" property="wantLook" />
    </resultMap>

</mapper>
